{
  "name": "Order Details Extension",
  "description": "Displays engraving details in the order admin",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://*.myshopify.com/admin/orders/*"],
      "js": ["order-details-extension.js"]
    }
  ],
  "permissions": ["storage"],
  "manifest_version": 3
}
